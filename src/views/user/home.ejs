<!-- ======================================= New Arrivals ================================================= -->

<section id="new-arrivals" class="new-arrival product-carousel py-5 position-relative overflow-hidden">
  <div class="container">
    <div class="d-flex flex-wrap justify-content-between align-items-center mt-5 mb-3">
      <h4 class="text-uppercase">Our New Arrivals</h4>
      <a href="/shop" class="btn-link">View All Products</a>
    </div>
    <div class="swiper product-swiper open-up" data-aos="zoom-out">
      <div class="swiper-wrapper d-flex">
        <% newArrivals.forEach(product => { %>
          <div class="swiper-slide">
            <div class="product-item image-zoom-effect link-effect">
              <div class="image-holder position-relative">
                <a href="/shop/product/<%= product._id %>">
                  <img src="<%= product.productImage[0] %>" alt="<%= product.productName %>" class="product-image img-fluid">
                </a>
                <button onclick="addToWishlist('<%= product._id %>',this)" class="product-wishlist-btn">
                  <svg xmlns="http://www.w3.org/2000/svg" width="20" height="20" fill="currentColor" class="bi bi-heart" viewBox="0 0 16 16">
                    <path d="m8 2.748-.717-.737C5.6.281 2.514.878 1.4 3.053c-.523 1.023-.641 2.5.314 4.385.92 1.815 2.834 3.989 6.286 6.357 3.452-2.368 5.365-4.542 6.286-6.357.955-1.886.838-3.362.314-4.385C13.486.878 10.4.28 8.717 2.01zM8 15C-7.333 4.868 3.279-3.04 7.824 1.143q.09.083.176.171a3 3 0 0 1 .176-.17C12.72-3.042 23.333 4.867 8 15"/>
                  </svg>
                </button>
                <div class="product-content">
                  <h5 class="text-uppercase fs-5 mt-3">
                    <a href="/product/<%= product._id %>"><%= product.productName %></a>
                  </h5>
                  <% if (product.salePrice < product.regularPrice) { %>
                    <a href="/cart/add/<%= product._id %>" class="text-decoration-none" data-after="Add to cart">
                      <span class="current-price">₹<%= product.salePrice.toFixed(2) %></span>
                      <span class="old-price text-decoration-line-through ms-2">₹<%= product.regularPrice.toFixed(2) %></span>
                    </a>
                  <% } else { %>
                    <a href="/cart/add/<%= product._id %>" class="text-decoration-none" data-after="Add to cart">
                      <span>₹<%= product.salePrice.toFixed(2) %></span>
                    </a>
                  <% } %>
                  <% if (product.quantity <= 0 || product.status === "out of stock") { %>
                    <div class="out-of-stock mt-2 text-danger">Out of Stock</div>
                  <% } %>
                </div>
              </div>
            </div>
          </div>
        <% }); %>

        <% if (newArrivals.length === 0) { %>
          <div class="swiper-slide">
            <div class="p-4 text-center">
              <p>No new arrivals at the moment.</p>
            </div>
          </div>
        <% } %>
      </div>
      <div class="swiper-pagination"></div>
    </div>
    <div class="icon-arrow icon-arrow-left"><svg width="50" height="50" viewBox="0 0 24 24">
        <use xlink:href="#arrow-left"></use>
      </svg></div>
    <div class="icon-arrow icon-arrow-right"><svg width="50" height="50" viewBox="0 0 24 24">
        <use xlink:href="#arrow-right"></use>
      </svg></div>
  </div>
</section>

